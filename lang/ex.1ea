time : 4/4
division: 1/16

pattern mypattern : 1 | 2 | 3 e + a | 4 e + a |

pattern mypattern2 : 1 1 2 3

bar mybar:
    cc: 1 |
    hh:  + | 2 e + a | 3 + | 4  + |
    sn:   | 2 |   | 4 |
    bd: 1 |   | 3 |   |

pippet mysnippet:
    repeat 2:
        mybar (2) {
            bd: 1 |  2 | 3 |  4 |
        }

    repeat 2:   
        mybar (2) {
            bd: 1 |  2 | 3 |  4 |
        }


render: mysnippet
