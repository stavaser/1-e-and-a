time : 4/4
division: 1/16

pattern mypattern : 1 | 2 | 3 e + a | 4 e + a |

pattern mypattern2 : 1 1 2 3

bar mybar:
    cc: 1 |
    hh:  + | 2 e + a | 3 + | 4  + |
    sn:   | 2 |   | 4 |
    bd: 1 |   | 3 |   |

bar mybar2:
    sn: 1 | 2 |3 |4 |
    t2: 1 e + a| 2 e + a|3 e + a|4 e + a|

pippet mysnippet:
    repeat 6: mybar (1,2,6) {
        bd: 1 e + a| 2 e + a|3 e + a|4 e + a|
    }


render: mysnippet
